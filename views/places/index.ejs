<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Bestpoints - Places</title>
  </head>
  <body>
    <h1>All Places</h1>
    <a href="/places/create">Add New Place</a>
    <ul>
      <% for (const place of places) { %>
      <li>
        <a href="/places/<%= place.id %>"> <%= place.title %> </a>
      </li>
      <% } %>
    </ul>
  </body>
</html>
